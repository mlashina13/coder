{
  "parser": "@typescript-eslint/parser",
  "env": {
    "jest": true
  },
  "extends": [
    "airbnb",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript",
    "prettier"
  ],
  "rules": {
    "no-path-concat": "off",
    "prefer-template": "off",
    "import/extensions": [
      "error",
      "never",
      {
        "scss": "always",
        "interface": "always"
      }
    ]
  }
}
